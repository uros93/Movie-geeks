

<%= form_tag(imdb_movies_index_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search Movies" %>
<%= submit_tag "Search" %>
<% end %>


<% @movies.each do |t| %>
<%= link_to t.title, imdb_movies_show_path(:movie_id => t.id) %><br>
<% end %>